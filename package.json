  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "json": "cd json-server && json-server --watch db.json --middlewares invitation.js login.js mailcheck.js",
    "json-ext": "cd json-server && json-server --watch db.json --middlewares invitation.js login.js mailcheck.js --host 192.168.1.115",
    "json-mobile": "cd json-server && json-server --watch db.json --middlewares m_invitation.js m_login.js m_mailcheck.js m_passcheck.js --host 192.168.137.1",
